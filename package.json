{
  "name": "@nekofar/gulp-fontiran",
  "version": "1.0.7",
  "description": "Download purchased fonts from Fontiran",
  "homepage": "https://github.com/nekofar/gulp-fontiran",
  "license": "MIT",
  "author": {
    "name": "Milad Nekofar",
    "email": "milad@nekofar.com",
    "url": "http://milad.nekofar.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:nekofar/gulp-fontiran.git"
  },
  "bugs": {
    "url": "https://github.com/nekofar/gulp-fontiran/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=8.16.0",
    "yarn": "^1.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rfv ./dist/",
    "lint": "eslint ./src/**/*.ts",
    "test": "yarn lint && nyc mocha -r ts-node/register test/**/*.ts",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "posttest": "yarn codecov"
  },
  "dependencies": {
    "async": "^3.0.1",
    "dotenv": "^8.0.0",
    "event-stream": "^4.0.1",
    "http-proxy-agent": "^3.0.0",
    "plugin-error": "latest",
    "request": "^2.88.0",
    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.1",
    "@types/chai": "^4.2.1",
    "@types/cypress": "^1.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/event-stream": "^3.3.34",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/request": "^2.48.2",
    "@types/vinyl": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "eslint": "^6.1.0",
    "eslint-config-gulp": "^3.0.1",
    "gulp": "^4.0.2",
    "mocha": "^6.2.0",
    "nyc": "^15.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.5.3",
    "typings": "^2.1.1"
  },
  "keywords": [
    "gulpplugin",
    "gulp",
    "font",
    "asset",
    "fontiran"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
